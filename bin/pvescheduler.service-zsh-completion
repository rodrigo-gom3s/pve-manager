#compdef _pvescheduler pvescheduler

function _pvescheduler() {
    local cwords line point cmd curr prev
    cwords=${#words[@]}
    line=$words
    point=${#line}
    cmd=${words[1]}
    curr=${words[cwords]}
    prev=${words[cwords-1]}
    compadd -- $(COMP_CWORD="$cwords" COMP_LINE="$line" COMP_POINT="$point" \
	pvescheduler bashcomplete "$cmd" "$curr" "$prev")
}
